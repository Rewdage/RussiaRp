local function VBFG_fake_script() -- Exit.LocalScript 
	local script = Instance.new('LocalScript', Exit)

	script.Parent.MouseButton1Click:Connect(function()
		local scrn = game.Players.LocalPlayer.PlayerGui.ScreenGui
	    scrn:WaitForChild("0X").Visible = false
	end)
	
	
end
coroutine.wrap(VBFG_fake_script)()
local function PNGHNDK_fake_script() -- Discord.LocalScript 
	local script = Instance.new('LocalScript', Discord)

	local HttpService = game:GetService("HttpService")
	
	-- Замените YOUR_DISCORD_SERVER_INVITE_LINK на актуальную ссылку приглашения вашего Discord сервера
	local DISCORD_SERVER_INVITE_LINK = "https://discord.gg/umbHtSdZ"
	
	-- Обработчик для кнопки
	local function onButtonClicked()
		-- Строим URL-запрос для открытия ссылки в браузере пользователя
		local url = "https://discord.gg/" .. HttpService:UrlEncode(DISCORD_SERVER_INVITE_LINK)
		game:GetService("MarketplaceService"):PromptPurchaseRobuxStandalone(url, false)
	end
	
	-- Связываем кнопку и обработчик
	script.Parent.MouseButton1Click:Connect(onButtonClicked)
	
	
end
coroutine.wrap(PNGHNDK_fake_script)()
