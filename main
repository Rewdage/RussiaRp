-- Scripts:

-- ScreenGui.DeleteAlt is disabled.
-- ScreenGui.Fly is disabled.
local function HUOHVD_fake_script() -- Fly.LocalScript 
	local script = Instance.new('LocalScript', Fly)

	local Button = script.Parent -- Получаем кнопку
	
	local isActivated = false -- Устанавливаем начальное значение флага, отвечающего за то, активен ли скрипт
	
	local function activateScript() -- Функция для активации скрипта
		isActivated = true -- Устанавливаем значение флага в "true"
		Button.BackgroundColor3 = Color3.new(0, 1, 0) -- Меняем цвет кнопки на зеленый
		-- Вставьте свой код, который нужно выполнить тут
	game.Players.LocalPlayer.PlayerGui.ScreenGui.Fly.Enabled = true
	end
	
	local function deactivateScript() -- Функция для деактивации скрипта
		isActivated = false -- Устанавливаем значение флага в "false"
		Button.BackgroundColor3 = Color3.new(1, 0, 0) -- Меняем цвет кнопки на красный
		-- Вставьте свой код, который нужно выполнить тут
		game.Players.LocalPlayer.PlayerGui.ScreenGui.Fly.Enabled = false
	end
	
	Button.Activated:Connect(function()
		if isActivated then
			deactivateScript() -- Если скрипт уже активирован, то его нужно деактивировать
		else
			activateScript() -- Если скрипт не активирован, то его нужно активировать
		end
	end)
end
coroutine.wrap(HUOHVD_fake_script)()
local function RYJPOGA_fake_script() -- _0X.Передвижение 
	local script = Instance.new('LocalScript', _0X)

	ui = script.Parent
	ui.Draggable = true
	ui.Active = true
	ui.Selectable = true
end
coroutine.wrap(RYJPOGA_fake_script)()
